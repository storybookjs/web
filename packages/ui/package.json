{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./src/main.ts"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@docsearch/react": "^3.6.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@repo/utils": "^0.0.0",
    "@storybook/icons": "^1.2.9",
    "copy-to-clipboard": "^3.3.3",
    "formik": "^2.4.6",
    "he": "^1.2.0",
    "next": "^14.2.3",
    "next-mdx-remote": "^5.0.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "snarkdown": "^2.0.0",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.2.0-next.1",
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@storybook/addon-themes": "^10.0.0-beta.10",
    "@storybook/nextjs": "^10.0.0-beta.10",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/he": "^1.2.3",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^10.0.0-beta.10",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "storybook": "^10.0.0-beta.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "@storybook/addon-docs": "^10.0.0-beta.10"
  }
}
